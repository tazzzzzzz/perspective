{
  "name": "rectify",
  "private": true,
  "scripts": {
    "start": "parcel src/index.html --no-autoinstall --open",
    "build": "npm run clean && parcel build src/index.html --public-url . --no-source-maps",
    "clean": "rm -rf dist/*",
    "deploy": "gh-pages -d dist"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "parcel-bundler": "^1.12.4"
  },
  "dependencies": {
    "gh-pages": "^2.2.0"
  }
}
